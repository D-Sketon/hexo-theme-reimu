vendors = official

@keyframes blur 
  0%
    filter: blur(10px)
  
  to
    filter: blur(0)

@keyframes blur-darken
  0%
    filter: blur(10px) brightness(70%)
  
  to
    filter: blur(0) brightness(70%)

@keyframes rotate-all
  100%
    transform: rotate(360deg)

@keyframes transform-all
  100%
    transform: translateX(-100px)